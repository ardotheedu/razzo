{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "no-unused-expressions": "off",
        "import/prefer-default-export": "off",
        "import/no-duplicates": "off",
        "camelcase": "off",
        "no-use-before-define": "off",
        "prettier/prettier": "error"
    }
}
